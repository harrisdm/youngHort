<h1>SlideshowImages#new</h1>

<%= form_for @image do |f| %>
  <div>
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>
  <div>
    <%= f.label :active %>
    <%= f.check_box :active, :checked => true %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>